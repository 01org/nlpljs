{
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "0.0.1.18",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "management",
    "declarativeContent",
    "tabs",
    "identity",
    "storage",
    "https://www.facebook.com/",
    "https://graph.facebook.com/",
    "<all_urls>"
  ],
  "page_action": {
    "default_icon": "images/icon-16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/*/document/*",
        "https://docs.google.com/document/*"
      ],
      "run_at": "document_start",
      "js": ["content_script/cs-rehost-page.js"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": [
      "event_page/ep-http.js",
      "event_page/ep-auth.js",
      "event_page/ep-sources-storage.js",
      "event_page/ep-main.js",
      "event_page/ep-nlp.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "",
    "/",
    "images/*",
    "content_push/*",
    "content_push/ClearSans/*",
    "content_push/styles/*",
    "styles/main.css",
    "bower_components/*/*",
    "libnlp/models/*"
  ],
  "oauth2": {
    "client_id": "375300658553-ollsgctiqracj6ab6r1oo4je1hjag7i4.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://docs.google.com/*/document/*",
      "https://docs.google.com/document/*"
    ]
  },
  "key": ""
}
